<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="premier.xml.player">
	<select id="selectAllPlayer" resultType="player">
		select * from player
	</select>
	<select id="selectOnePlayer" parameterType="java.lang.String" resultType="player">
		select * from player
		where playerNo = #{playerNo}
	</select>
</mapper>